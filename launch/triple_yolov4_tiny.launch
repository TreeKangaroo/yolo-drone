<launch>
	
	<!-- yolov4_trt_node -->
	<node pkg="yolov4_trt_ros" type="yolo_tiny_416_leader_test.py" name="yolov4_trt_leader" output="screen">		
		<param name="video_topic" type="string" value="/video_source/raw1" />
		<param name="model" type="string" value="yolov4-tiny" />	
		<param name="model_path" type="string" value="$(find yolov4_trt_ros)/yolo/" />
		<param name="input_shape" type="string" value="416" />
		<param name="category_number" type="int" value="80" />
		<param name="confidence_threshold" type="double" value="0.5" />
		<param name="show_image" type="bool" value="True" />
	</node>
	
	<!-- yolov4_trt_node -->
	<node pkg="yolov4_trt_ros" type="tello_script0.py" name="wingman0_node" output="screen">		
		<param name="video_topic" type="string" value="/video_source/raw" />
		<param name="model" type="string" value="yolov4-tiny-416" />	
		<param name="model_path" type="string" value="$(find yolov4_trt_ros)/yolo/" />
		<param name="input_shape" type="string" value="416" />
		<param name="category_number" type="int" value="80" />
		<param name="confidence_threshold" type="double" value="0.5" />
		<param name="show_image" type="bool" value="True" />
	</node>
	
	<!-- yolov4_trt_node -->
	<node pkg="yolov4_trt_ros" type="tello_script1.py" name="wingman1_node" output="screen">		
		<param name="video_topic" type="string" value="/video_source/raw" />
		<param name="model" type="string" value="yolov4-tiny-416" />	
		<param name="model_path" type="string" value="$(find yolov4_trt_ros)/yolo/" />
		<param name="input_shape" type="string" value="416" />
		<param name="category_number" type="int" value="80" />
		<param name="confidence_threshold" type="double" value="0.5" />
		<param name="show_image" type="bool" value="True" />
	</node>
	
</launch>
